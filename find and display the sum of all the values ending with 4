#include<iostream>
using namespace std;
int main()
{
	int i,num,count=0,sum=0,a;
	for(i=1;i<=14;i++)
	{
		if(i%10==4)
		{
			cout<<"number ending with 4 are: "<<i<<"\n";
			sum=sum+i;
			count=count+1;
		}
   }
   cout<<" no.of numbers ending with 4 are:"<<count<<"\n";
   cout<<"sum of the numbers is"<<sum;
   return 0;
   
}
